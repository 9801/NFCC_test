/* pages/set/set.wxss */

.container01 view {
  width: 100%;
  height: 100%;
  text-align: center;
  line-height: 100px;
}
.container01 view:nth-child(1){
  background-color: aqua;
}
.container01 view:nth-child(2){
  background-color: #faceff;
}
.container01 view:nth-child(3){
  background-color: darkgoldenrod;
}

.container01 view:nth-child(4){
  background-color: blueviolet;
}

.container01 view:nth-child(5){
  background-color: crimson;
}

.container01 view:nth-child(6){
  background-color: darkgreen;
}

.container01 view:nth-last-child(2){
  background-color: white;
}

.container01 {
  /* display: flex;
  justify-content: space-around; */
  border: 1px maroon solid;
  width: 100%;
  height: 100%;
}